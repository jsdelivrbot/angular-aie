export class AieMisc {
  constructor(
    public startDate: string = "",         // defined in daterangepicker
    public endDate: string = "",           // defined in daterangepicker
    public srchRadio: string = "",
    public srchChk: string = "",
    public srchAmt: number = 0,
    public fct_sales_code: string = "",
    public fct_cost_account: number = 0,
    public fct_product_desc: string = "",
    public fct_odometer: number = 0,         // scOdoRead
    public fct_trans_date: number = 0,
    public fct_po_number: string = "",
    public fct_total_cost: number = 0,
    public fct_aie_amount: number = 0,
    public aie_credit: string = "",          // screen field not in DTO
    public fct_aie_status: string = "",      // scSelect
    public fct_key: number = 0,              // hidden fields
    public fct_billback_desc: string = "",   // Split 100 into 4 X 25 for DC-DESC1-4
    public fct_toUpdate: string = "",
    public scNegative: string = "",          // screen field hidden not in DTO
    public fct_orig_sales: string = "",      // screen field hidden not in DTO
    public fct_orig_cost: number = 0,        // screen field hidden not in DTO
    public fct_dc_update: string = "",       // screen field hidden not in DTO
    public fct_orig_status: string = "",     // screen field hidden not in DTO
    public fct_dc_found: string = "",        // screen field hidden not in DTO
    public fct_vh_agency_cl: string = "",
    public fct_vh_tag: string = "",
    public fct_cu_region: number = 0,  
    public fct_cu_boac: string = "",
    public po_number: string = "",
    public fct_dc_lid: string = ""
         
    ) { }
 };

