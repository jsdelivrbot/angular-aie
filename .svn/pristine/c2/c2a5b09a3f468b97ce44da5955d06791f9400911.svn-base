<form [formGroup]="tagSearchForm" >
<div class="panel panel-default">
  <div class="panel-body">
    <div class="form-group form-group5 form-inline">
      
      <div class=col-md-12>
        <label class="control-label">Tag Number:&nbsp;&nbsp;G</label>&nbsp;
        <input class="form-control" size="2" maxlength="2" type="text" formControlName="agencyCl" />  
        <label class="control-label"> &nbsp; - &nbsp;</label>
        <input class="form-control" size="5" maxlength="5" type="text" formControlName="tag" />
        
        <button type="submit" [disabled]="!tagSearchForm.valid" class="btn btn-dt2 btn-sm" (click)="findCustVehByTagSubmit(loadingModal, tagSearchForm)">Submit</button>
        <button type="button" (click)="clearFields()" class="btn btn-dt2 btn-sm">Clear</button>
        
      </div>  
      <div class="clearfix"></div>
      
      <div class="col-md-4"> &nbsp;</div>
      <alert *ngIf="this.errorString.length > 0"  type="danger" class="col-md-4"> 
      <!-- <alert *ngIf="errorString.length > 0"  type="warning" class="col-md-4">  -->
       {{ errorString }}
      </alert>
      <div class="col-md-4"> &nbsp;</div>
      <div class="clearfix"></div>
      
    </div>

    <div class="clearfix">&nbsp;</div>
    <div class="col-md-4">
      <div class="form-horizontal">
        <div class="form-group3"><label class="col-sm-5 control-label">Customer Number:</label><div class="col-sm-7"><div class="form-control-static1">
          {{ customer.cu_Region | number: '2.0-0' }}-{{ customer.fmc | number: '2.0-0' }}-{{ customer.subFMC | number: '2.0-0' }}-{{ customer.boac }}-{{ customer.serial | number: '3.0-0' }} &nbsp;
        </div></div></div>
        <div class="clearfix"></div>
        <div class="form-group3"><label class="col-sm-5 control-label">Customer Name:</label><div class="col-sm-7"><div class="form-control-static1">    {{ customer.contact }} &nbsp;</div></div></div>
        <div class="clearfix"></div>
		<div class="form-group3"><label class="col-sm-5 control-label">Fund Code:</label><div class="col-sm-7"><div class="form-control-static1">        {{ vehicle.fund }} &nbsp;</div></div></div>
		<div class="clearfix"></div>
        <div class="form-group3"><label class="col-sm-5 control-label">Acct 1:</label><div class="col-sm-7"><div class="form-control-static1">           {{ vehicle.acct1 }} &nbsp;</div></div></div></div>
    </div>
    <div class="col-md-4">
      <div class="form-horizontal">
        <div class="form-group3"><label class="col-sm-5 control-label">Manufactuer Name:</label><div class="col-sm-7"><div class="form-control-static1"> {{ vehicle.manufacturerCode }} &nbsp;</div></div></div>
        <div class="clearfix"></div>
        <div class="form-group3"><label class="col-sm-5 control-label">Model Year:</label><div class="col-sm-7"><div class="form-control-static1">       {{ vehicle.year }} &nbsp;</div></div></div>
        <div class="clearfix"></div>
        <div class="form-group3"><label class="col-sm-5 control-label">Model Name:</label><div class="col-sm-7"><div class="form-control-static1">       {{ vehicle.model }} &nbsp;</div></div></div>
        <div class="clearfix"></div>
        <div class="form-group3"><label class="col-sm-5 control-label">Acct 2:</label><div class="col-sm-7"><div class="form-control-static1">           {{ vehicle.acct2 }} &nbsp;</div></div></div>
      </div>
    </div>
    <div class="col-md-4">
      <div class="form-horizontal">
        <div class="form-group3"><label class="col-sm-5 control-label">Inventory Status:</label><div class="col-sm-7"><div class="form-control-static1"> {{ vehicle.InvStatus }} &nbsp;</div></div></div>
        <div class="clearfix"></div>
        <div class="form-group3"><label class="col-sm-5 control-label">Fuel Name:</label><div class="col-sm-7"><div class="form-control-static1">        {{ vehicle.fuelType }} &nbsp;</div></div></div>
        <div class="clearfix"></div>
        <div class="form-group3"><label class="col-sm-5 control-label">Fuel Tank Size:</label><div class="col-sm-7"><div class="form-control-static1">   {{ vehicle.tankSize   }} &nbsp;</div></div></div>
        <div class="clearfix"></div>
        <div class="form-group3"><label class="col-sm-5 control-label">&nbsp;</label><div class="col-sm-7">&nbsp;</div></div>
      </div>
    </div>
  </div>
</div>

</form>

<app-loading-bar></app-loading-bar>

